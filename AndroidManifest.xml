<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.logictree.commonwealth"
    android:versionCode="1"
    
    android:versionName="1.0" android:installLocation="preferExternal">

    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" android:theme="@android:style/Theme.Light.NoTitleBar">
        <activity android:name=".activities.ContactUsActivity"></activity>
        <activity android:name=".activities.EventsActivity"></activity>
        <activity android:name=".activities.EventsDetailsActivity"></activity>
        <activity android:name=".activities.HomeActivity"></activity>
        <activity android:name=".activities.SplashScreenActivity"><intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter></activity>
    </application>

</manifest>